<UserControl x:Class="YoutubeAI.Carro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YoutubeAI"
             mc:Ignorable="d" 
             Height="10" Width="10">
    <Grid x:Name="eu" RenderTransformOrigin="0.5,0.5" Background="Yellow">
        <Grid.RenderTransform>
            <TransformGroup>
                <RotateTransform x:Name="meuAngulo" Angle="0"></RotateTransform>
            </TransformGroup>
        </Grid.RenderTransform>

        <Line X1="0" Y1="2" X2="10" Y2="5" Stroke="Blue"></Line>
        <Line X1="0" Y1="8" X2="10" Y2="5" Stroke="Blue"></Line>
        
    </Grid>
</UserControl>
